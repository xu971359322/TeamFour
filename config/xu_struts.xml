<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
    <package name="service" namespace="/xu" extends="struts-default">
        <action name="serv_*" class="org.java.web.ServiceAction" method="{1}">
            <result name="addservice">setService.jsp</result>
            <result name="give">giveService.jsp</result>
            <result name="dispose">dispose.jsp</result>
        </action>

    </package>

</struts>